<div class="app-content content">
  <div class="content-wrapper">
    <div class="content-body"> 
      <!-- Basic form layout section start -->
      <section id="configuration" class="search view-cause">
        <div class="row">
          <div class="col-12">
            <div class="card rounded pad-20">
              <div class="card-content collapse show">
                <div class="card-body table-responsive card-dashboard">
                  <div class="row">
                    <div class="col-6">
                      <h1 class="pull-left" style="padding-left:15px;">USers listing</h1>
                    </div>
                    <div class="col-6">
                      <div class="add-user text-right"> <a href="/adduser" class="user"><i class="fa fa-plus-circle"></i> add user</a> </div>
                    </div>
                  </div>
					<form action="/users/create">
					<div class="row">
					<div class="offset-lg-6  col-lg-3 col-md-6 col-12">
						<input id="datepicker" class="form-control" placeholder="search by date" />
						</div>
						<div class="col-lg-3 col-md-6 col-12">
						<select name="" class="form-control" id="">
							<option value="">search by name</option>
							<option value="">search by name</option>
						</select>
						</div>
					</div>
						</form> 
                  <div class="clearfix"></div>
                  <div class="maain-tabble">
                    <table class="table table-striped table-bordered zero-configuration">
                      <thead>
                        <tr>
                          <th>Name</th>
                          <th>Email</th>
                          <th>Phone</th>
                          <th>Registered Since</th>
                          <th>Current Status</th>
						  <th>Total Alerts</th>
						  <th>action</th>
                        </tr>
                      </thead>
                      <tbody>
                        <% users.forEach(function(user){ %>
                        <tr>
 
                          <td><span data-toggle="popover" data-content="johny" class="circle" style="background: #f61454;"><%= user.name %></span></td>
                          <td> <%= user.email %></td>
                          <td> <%= user.phoneno %></td>
						  <td>15 Jan 2019</td>
                          <td><p class="active">active</p></td>
                          <td>54</td>
                          <td><div class="btn-group mr-1 mb-1">
                              <button type="button" class="btn  btn-drop-table btn-sm" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"> <i class="fa fa-ellipsis-v"></i></button>
                              <div class="dropdown-menu" x-placement="bottom-start" style="position: absolute; transform: translate3d(0px, 21px, 0px); top: 0px; left: 0px; will-change: transform;">  <a class="dropdown-item" href="user-detail.html"><i class="fa fa-eye"></i>View </a> <a class="dropdown-item" href="#"><i class="fa fa-trash"></i> Delete</a> </div>
                            </div></td>
                        </tr>
                        <% }) %>
                        
                      </tbody>
                    </table>
					  
					   
                  </div>
                </div>
              </div>
              <!--main card end--> 
              
            </div>
          </div>
        </div>
      </section>
      <!-- // Basic form layout section end --> 
    </div>
  </div>
</div> 

