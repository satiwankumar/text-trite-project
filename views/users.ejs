<div class="main-menu menu-fixed menu-light menu-accordion" data-scroll-to-active="true">
  <div class="main-menu-content">
    <ul class="navigation navigation-main" id="main-menu-navigation" data-menu="menu-navigation">  
      <li class="nav-item active"><a href="/users"><i class="fa fa-user"></i><span class="menu-title" data-i18n="">users</span></a></li>
      <li class="nav-item "><a href="/user/feeback"><i class="fa fa-comments"></i><span class="menu-title" data-i18n="">Feedback</span></a></li>
    </ul>
  </div>
