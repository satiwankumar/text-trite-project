<body class="vertical-layout vertical-menu 2-columns   menu-expanded fixed-navbar" data-open="click"
  data-menu="vertical-menu" data-col="2-columns">
  <!-- fixed-top-->
  <%- partial('./partials/Nav.ejs') %>

  <!-- ////////////////////////////////////////////////////////////////////////////-->
 
  <div class="app-content content">
    <div class="content-wrapper">
      <div class="content-body">
        <!-- Basic form layout section start -->
        <section id="configuration" class="search view-cause">
          <div class="row">
            <div class="col-12">
              <div class="card rounded pad-20">
                <div class="card-content collapse show">
                  <div class="card-body table-responsive card-dashboard">
                    <div class="row">
                      <div class="col-12">
                        <h1 class="pull-left" style="padding-left:15px;">Feedback list</h1>
                      </div>
                    </div>
                    <div class="row">
                      <div class="offset-lg-9 offset-md-6 col-lg-3 col-md-6 col-12">
                        <input id="datepicker" class="form-control" placeholder="search by date" />
                      </div>
                    </div>
                    <div class="clearfix"></div>
                    <div class="maain-tabble">
                      <table class="table table-striped table-bordered zero-configuration">
                        <thead>
                          <tr>
                            <th>from</th>
                            <th>subject</th>
                            <th>description</th>
                            <th>Date </th>
                            <th>Action</th>
                          </tr>
                        </thead>
                        <tbody>
                          <% ListFeedback.forEach(function(feedback){ %>
                          <tr>
                            <td><span data-toggle="popover" data-content="johny" ><%= feedback.user.name%></span> </td>
                           
                            <td><%= feedback.subject%> </td>
                            <td><%= feedback.description%> </td>
                            <td><%= feedback.createdAt %></td>
                            <td>
                              <div class="btn-group mr-1 mb-1">
                                <button type="button" class="btn  btn-drop-table btn-sm" data-toggle="dropdown"
                                  aria-haspopup="true" aria-expanded="false"> <i class="fa fa-ellipsis-v"></i></button>
                                <div class="dropdown-menu" x-placement="bottom-start"
                                  style="position: absolute; transform: translate3d(0px, 21px, 0px); top: 0px; left: 0px; will-change: transform;">
                                  <a class="dropdown-item" href="/user/feedback/edit/<%=feedback.id%>"><i class="fa fa-eye"></i>View
                                  </a> 
                                
                                  <form class="d-inline" action="/user/feedback/delete/<%= feedback.id %>" method="post">
                                    
                                    <input type="submit" class="dropdown-item"  value="Delete" >
                                    <i class="d-inline fa fa-trash"></i> 
                                  </input>
                                </form>

                                </div>
                              </div>
                            </td>
                          </tr>

                          <% }) %>
                        </tbody>
                      </table>
                    </div>
                  </div>
                </div>
                <!--main card end-->

              </div>
            </div>
          </div>
        </section>
        <!-- // Basic form layout section end -->
      </div>
    </div>
  </div>


</body>

</html>